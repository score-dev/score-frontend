<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/btn_start_kakao"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_start_kakao"
        app:layout_constraintBottom_toTopOf="@id/btn_start_google"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="10dp"/>

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/btn_start_google"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_start_google"
        app:layout_constraintBottom_toTopOf="@+id/btn_start_naver"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="10dp"/>

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/btn_start_naver"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_start_naver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="10dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>